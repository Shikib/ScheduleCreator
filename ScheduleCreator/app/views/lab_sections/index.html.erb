<h1>Listing lab_sections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lab_sections.each do |lab_section| %>
      <tr>
        <td><%= link_to 'Show', lab_section %></td>
        <td><%= link_to 'Edit', edit_lab_section_path(lab_section) %></td>
        <td><%= link_to 'Destroy', lab_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lab section', new_lab_section_path %>
