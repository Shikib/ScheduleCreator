<h1>Listing tutorial_sections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutorial_sections.each do |tutorial_section| %>
      <tr>
        <td><%= link_to 'Show', tutorial_section %></td>
        <td><%= link_to 'Edit', edit_tutorial_section_path(tutorial_section) %></td>
        <td><%= link_to 'Destroy', tutorial_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutorial section', new_tutorial_section_path %>
